{
  "properties": {
    "displayName": "Deny Public IP",
    "description": "Prevent creation of Public IP addresses",
    "mode": "All",
    "policyRule": {
      "if": {
        "type": "Microsoft.Network/publicIPAddresses"
      },
      "then": {
        "effect": "deny"
      }
    }
  }
}
